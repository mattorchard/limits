{
  "name": "Limits",
  "version": "0.0.0.1",
  "description": "Adds time limits to certain pages",
  "icons": {
    "128": "Limits-128.png",
    "512": "Limits-512.png"
  },
  "background": {
    "scripts":["background.js"]
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "*://*/*"
      ],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "storage", "tabs"
  ],
  "browser_action": {
    "default_icon": "Limits-128.png"
  },
  "manifest_version": 2
}