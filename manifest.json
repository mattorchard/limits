{
  "name": "Limits",
  "version": "0.0.0.3",
  "description": "Adds time limits to certain pages",
  "icons": {
    "128": "Limits-128.png",
    "512": "Limits-512.png"
  },
  "options_page": "options.html",
  "background": {
    "scripts":["background.js"]
  },
  "permissions": [
    "storage", "tabs"
  ],
  "browser_action": {
    "default_icon": "Limits-128.png"
  },
  "manifest_version": 2
}